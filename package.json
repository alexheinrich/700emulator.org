{
  "name": "700emulator",
  "version": "0.0.1",
  "description": "An modern approximation of the Buchla 700.",
  "scripts": {
    "build": "eleventy --pathprefix=/700emulator.org",
    "watch": "eleventy --watch",
    "serve": "webpack --mode development --watch & eleventy --serve",
    "serve:webpack": "webpack --mode development --watch",
    "serve:eleventy": "eleventy --serve",
    "start": "eleventy --serve",
    "debug": "DEBUG=* eleventy",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d _site"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:alexheinrich/700emulator.org.git"
  },
  "author": {
    "name": "Alexander Heinrich",
    "email": "alex.heinrich@icloud.com",
    "url": "https://700emulator.org"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexheinrich/700emulator.org.git"
  },
  "homepage": "https://github.com/alexheinrich/700emulator.org.git",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "gh-pages": "^3.1.0",
    "luxon": "^1.21.3",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.2.5",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "webpack": "^5.3.0",
    "webpack-cli": "^4.1.0"
  },
  "dependencies": {
    "eleventy-plugin-sass": "^1.1.1",
    "reset-css": "^5.0.1",
    "typed.js": "^2.0.11"
  }
}
